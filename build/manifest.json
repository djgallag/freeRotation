{
  "name": "Free Rotation",
  "version": "1.0",
  "description": "Inform the user as to which League of Legends Champions are on free Rotation",
  "permissions": [
    "http://localhost:3000/"
  ],
  "background": {
    "scripts": [
      "app.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "./index.html",
    "default_icon": {
      "16": "./images/league16.png",
      "32": "./images/league32.png",
      "48": "./images/league48.png",
      "128": "./images/league128.png"
    }
  },
  "icons": {
    "16": "./images/league16.png",
    "32": "./images/league32.png",
    "48": "./images/league48.png",
    "128": "./images/league128.png"
  },
  "manifest_version": 2
}